{
   "background": {
      "scripts": [ "jquery-3.2.1.js", "tipped/tipped.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "32": "images/load.png"
      },
      "default_title": "Click to Toggle"
   },
   "content_scripts": [ {
      "css": [ "tipped/tipped.css" ],
      "js": [ "jquery-3.2.1.js", "tipped/tipped.js", "tooltip.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "description": "Adds informative tooltips to University of Toronto courses mentioned across the web",
   "icons": {
      "128": "images/icon128.png",
      "48": "images/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "UofT Course Info",
   "permissions": [ "activeTab", "*://*/*", "tabs", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3",
   "options_page": "settings/settings.html",
   "web_accessible_resources": ["tipped/tipped.css", "directory.json", "settings, textbooks, mapper"],
   "content_security_policy": "script-src 'self' https://code.jquery.com https://ajax.googleapis.com https://cdnjs.cloudflare.com; object-src 'self' https://code.jquery.com; style-src 'self' 'unsafe-inline' https://code.jquery.com"

}
